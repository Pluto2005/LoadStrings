            end
        end
    end
end)

WorldSection:NewToggle("Lava Water", "Makes Water like Lava", function(value)
    for i,v in next,game.Workspace.Water:GetChildren() do
        if v.Name == "Water" then
            if value then
                v.Color = Color3.fromRGB(255,0,0)
                v.Material = "Granite"
                else
                    v.Color = Color3.fromRGB(0, 143, 156)
                    v.Material = "SmoothPlastic"
            end
        end
    end
end)

WorldSection:NewToggle("Heaven Water", "Heaven Water", function(value)
    for i,v in next,game.Workspace.Water:GetChildren() do
        if v.Name == "Water" then
            if value then
                v.Color = Color3.fromRGB(255, 255, 255)
                Brightness(value)
                else
                    v.Color = Color3.fromRGB(0, 143, 156)
            end
        end
    end
end)


